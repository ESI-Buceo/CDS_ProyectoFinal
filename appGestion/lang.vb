    Public Sub GuardarIdioma(ByVal idioma As String)
        If idioma.Equals("Español") Or idioma.Equals("Spanish") Then
            My.Settings.lenguaje = "es"
        Else
            My.Settings.lenguaje = "en"
        End If
        My.Settings.Save()
        My.Settings.Reload()
    End Sub
